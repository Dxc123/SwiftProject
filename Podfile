# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'
source 'https://github.com/CocoaPods/Specs.git'
target 'SwiftProject' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SwiftProject

    pod 'Alamofire’#网络请求
    pod 'Alamofire-SwiftyJSON' #Alamofire扩展
    pod 'Moya'
   
    
    pod 'HandyJSON’#json 转model
    pod 'SwiftyJSON'#json 转model
    pod 'ObjectMapper'#数据模型转换
    
    pod 'PKHUD' #swift版Hud
    pod 'SnapKit'#代码布局
    pod 'Kingfisher'#网络图片缓存库
    pod 'ReachabilitySwift' #检查网络
    pod 'Reusable'#重用视图(UITableViewCells, UICollectionViewCells等）
    pod 'Then'#swift初始化库
    pod 'MBProgressHUD'#swift初始化库


  target 'SwiftProjectTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SwiftProjectUITests' do
    # Pods for testing
  end

end
